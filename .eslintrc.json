{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "linebreak-style": 0,
    "arrow-parens": 0,
    "jsx-a11y/accessible-emoji": 0,
    "react/jsx-one-expression-per-line": 0,
    "no-shadow": 0,
    "import/no-unresolved": 0
  }
}
